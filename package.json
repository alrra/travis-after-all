    "babel-preset-es2015": "^6.14.0",
    "babel-register": "^6.14.0",
    "babel-runtime": "^6.11.6",
    "tap": "^7.1.0",
    "tap-mocha-reporter": "^2.0.1"
  },
  "files": [
    "bin",
    "lib"
  ],
  "homepage": "https://github.com/alrra/travis-after-all#readme",
  "keywords": [
    "travis",
    "travis-ci"
  ],
  "license": "MIT",
  "main": "lib/travis-after-all.js",
  "name": "travis-after-all",
