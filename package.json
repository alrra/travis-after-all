{
  "author": "Cătălin Mariș <alrraa@gmail.com> (http://catalinmaris.com/)",
  "babel": {
    "plugins": [
      "transform-async-to-generator",
      "transform-runtime"
    ],
    "presets": [
      "es2015"
    ]
